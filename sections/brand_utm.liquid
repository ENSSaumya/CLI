<div class="brand_main">
  {% for block in section.blocks %}
  {% if block.type == 'image_picker' %}
  <div class="header_nav_brand" style=" background-color:{{ block.settings.background_header }}; padding-top:{{ block.settings.padding_top }}px; padding-bottom:{{ block.settings.padding_bottom }}px;">
    {% if block.settings.brand_logo != blank %}
    <div class="brand_logo">
      <img src="{{ block.settings.brand_logo | img_url:'master' }}" width="" height="" alt="{{ block.settings.brand_logo.alt }}" />
    </div>
    {% endif %}
    <ul class="header_nav_menu_list">
    {% for link in block.settings.header_menu.links %}
      {% if link.url != blank %} <a href="{{ link.url }}"><li style="color: {{block.settings.font_color_header}};">{{ link.title }}</li></a> {% endif %}
    {% endfor %}
    <li>
      {% if block.settings.cart_icon != blank %}<span style="order:{{ block.settings.position_cart }};">{{ block.settings.cart_icon }}</span> {% endif %}
      {% if block.settings.track_icon != blank %}<span style="order:{{ block.settings.position_track }};">{{ block.settings.track_icon }}</span> {% endif %}
      {% if block.settings.human_icon != blank %}<span style="order:{{ block.settings.position_login }};">{{ block.settings.human_icon }}</span> {% endif %}
    </li>
    </ul>
  </div>
  {% endif %}
  {% if block.type == 'text' %}
    <div class=footer_nav_brand style="background: {{ block.settings.footer_background }}; margin-top: {{ block.settings.margin_top }}px; margin-bottom: {{ block.settings.margin_bottom }}px;">
      {% if block.settings.footer_logo_brd != blank %}
        <div class="footer_brand_logo">
          <img src="{{ block.settings.footer_logo_brd | img_url:'master' }}" width="" height="" alt="{{ block.settings.footer_logo_brd.alt }}"/>
          <p style=" color: {{ block.settings.footer_font_color }}; font-size: {{ block.settings.font_size_footer }}px;">{{ block.settings.address_brand }}</p>
        </div>
      {% endif %}
      <span class="footer_nav_menu_list">
        <ul class="footer_list_1">
        {% for link in block.settings.footer_list1.links %}
          <a href="{{ link.url }}"><li style="color: {{ block.settings.footer_font_color }}; font-size: {{ block.settings.font_size_footer }}px;">{{ link.title }}</li></a>
        {% endfor %}
        </ul>
        <ul class="footer_list_2">
        {% for link in block.settings.footer_list2.links %}
          <a href="{{ link.url }}"><li style="color: {{ block.settings.footer_font_color }}; font-size: {{ block.settings.font_size_footer }}px;">{{ link.title }}</li></a>
        {% endfor %}
        </ul>
        <ul class="footer_list_3">
        {% for link in block.settings.footer_list3.links %}
          <a href="{{ link.url }}"><li style="color: {{ block.settings.footer_font_color }}; font-size: {{ block.settings.font_size_footer }}px;">{{ link.title }}</li></a>
        {% endfor %}
        </ul>
      </span>
      {% if block.settings.contact_detail_footer != blank %}
        <div class="contact_detail">
          <p>{{ block.settings.contact_detail_footer }}</p>
        </div>
      {% endif %}
    </div>
  {% endif %}
  {% endfor %}
</div>
{% schema %}
  {
    "name": "brand_utm",
    "settings": [],
    "blocks": [{
      "type":"image_picker",
      "name":"Header",
      "settings": [
        {
          "type": "range",
          "id": "padding_top",
          "min": 10,
          "max": 80,
          "step": 2,
          "unit": "px",
          "label": "Padding Top",
