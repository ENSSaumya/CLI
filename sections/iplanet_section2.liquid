<div class="main_iplanet2">
  <div class="content_iplanet2">
      <div class="image_iplanet2">
        <img src="{{ section.settings.img_iplanet2 | img_url:"" }}" >
      </div>
      <div class="enterprise_iplanet2">
      <div class="dash_enterprise">
      </div>
      <div class="heading_enterprise">
        <h2>{{ section.settings.heading1_iplanet2 }}</h2>
      </div>
      <div class="text_enterprise">
        <p>{{ section.settings.paragraph_iplanet6 }}</p>
      </div>
      <div class="script_enterprise">
        <div class="countdown1_script">
          <span id="count1">0</span>
        </div>
        <div class="text1_script">
          <p>Years of Excellence</p>
        </div>
        <div class="countdown2_script">
          <span id="count2">0</span><span id="plus_icon">+</span>
        </div>
        <div class="text2_script">
          <p>Apple Experts</p>
        </div>
      </div>
    </div>
  </div>
  <section class="authorised_iplanet2">
    <div class="text_authorised">
      <p>Explore, evaluate and deploy technology solutions for work â€” from office, from home and on the go. We have the knowledge, insights and best practices to ease the transformation, from technology, content and people management perspectives.</p>
    </div>
    <div class="symbol_authorised">
      <img style="width:358px;height:211px;" src="https://iplanet.one/wp-content/uploads/2022/03/Apple-Authorised-Reseller.png">
    </div>
  </section>
</div>

{% schema %}
  {
    "name": "iplanet2",
    "settings":[{
        "type":"image_picker",
        "id":"img_iplanet2",
        "label":"Image"
      },
      {
        "type":"text",
        "id":"heading1_iplanet2",
        "label":"Title"
      },
      {
        "type":"html",
        "id":"paragraph_iplanet6",
        "label":"Content"
      }
    ],
    "presets":[{"name":"iplanet2"}]
  }
{% endschema %}

