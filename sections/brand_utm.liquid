        "label":"Brand Logo"
      },
      {
        "type":"link_list",
        "id":"header_menu",
        "label":"Header Menu List"
      },
      {
        "type":"color",
        "id":"background_header",
        "label":"Background Color Header"
      },
      {
        "type":"color",
        "id":"font_color_header",
        "label":"Font Color Header"
      },
      {
        "type":"html",
        "id":"cart_icon",
        "label": "Cart"
      },
      {
        "type":"text",
        "id":"position_cart",
        "label": "Position of Cart"
      },
      {
        "type":"html",
        "id":"human_icon",
        "label":"Login/Register"
      },
      {
        "type":"text",
        "id":"position_login",
        "label": "Position of Login/Register"
      },
      {
        "type":"html",
        "id":"track_icon",
        "label":"Track icon"
      },
      {
        "type":"text",
        "id":"position_track",
        "label": "Position of Track Order"
      }]
    },
    {
      "type":"text",
      "name":"Footer",
      "settings": [
      {
        "type": "range",
        "id": "margin_top",
        "min": 10,
        "max": 80,
        "step": 2,
        "unit": "px",
        "label": "Margin Top",
        "default": 16
      },
      {
        "type": "range",
        "id": "margin_bottom",
        "min": 5,
        "max": 100,
        "step": 5,
        "unit": "px",
        "label": "Margin Bottom",
        "default": 5
      },
      {
        "type": "color",
        "id":"footer_background",
        "label": "Footer Background Color"
      },
      {
        "type": "color",
        "id":"footer_font_color",
        "label": "Footer Font Color"
      },
      {
        "type": "text",
        "id":"font_size_footer",
        "label": "Footer Font Size"
      },
      {
        "type": "image_picker",
        "id":"footer_logo_brd",
        "label": "Footer Logo Brand"
      },
      {
        "type": "richtext",
        "id":"address_brand",
        "label": "Address Brand"
      },
     {
        "type": "text",
        "id":"position_footer_1",
        "label": "Position Footer 1"
      },
     {
        "type": "link_list",
        "id":"footer_list1",
        "label": "Footer Link 1"
      },
     {
        "type": "text",
        "id":"position_footer_2",
        "label": "Position Footer 2"
      },
     {
        "type": "link_list",
        "id":"footer_list2",
        "label": "Footer Link 2"
      },
     {
        "type": "text",
        "id":"position_footer_3",
        "label": "Position footer 3"
      },
      {
        "type": "link_list",
        "id":"footer_list3",
        "label": "Footer Link 3"
      },
       {
        "type": "text",
        "id":"position_footer_4",
        "label": "Position Footer 4"
      },
      {
        "type": "html",
        "id":"contact_detail_footer",
        "label": "Contact Details"
      },
     {
        "type": "text",
        "id":"position_footer_5",
        "label": "Position Footer 5"
      }]
    }],
    "presets": [{"name":"brand_utm"}]
  }
{% endschema %}

