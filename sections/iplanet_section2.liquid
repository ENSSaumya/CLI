<script>
var time_initial = 0;
  function countdown1() {
      time_initial++;
      document.getElementById("count1").innerHTML = String(time_initial);
      if (time_initial <= 10) { 
          setTimeout(countdown1, 155); 
      }
  };
var time_left = 0;
  function countdown2(){
    time_left++;
    document.getElementById("count2").innerHTML = String(time_left);
    if (time_left < 19){
      setTimeout(countdown2, 70);
    }
  };
window.onload = function() {
    setTimeout(countdown1, 2000);
    setTimeout(countdown2, 2000);
};
</script>